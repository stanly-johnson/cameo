{
  "manifest_version": 2,

  "name": "Cameo",
  "description": "Ceramic DID Wallet for Chrome",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Cameo"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-AeVIsVsb..'; object-src 'self'", 
  "permissions": ["activeTab", "<all_urls>"]
}